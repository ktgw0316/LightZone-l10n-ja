msgstr ""

#. type: Bullet: '2.  '
#: Styles-Importing.markdown.txt:18
msgid "Click Import..."
msgstr ""

#. type: Bullet: '3.  '
#: Styles-Importing.markdown.txt:18
msgid "Navigate to the style file you want to import."
msgstr ""

#. type: Bullet: '4.  '
#: Styles-Importing.markdown.txt:18
msgid "Click Choose. (The style will be imported.)"
msgstr ""

